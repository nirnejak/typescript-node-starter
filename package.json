{
  "name": "typescript-node-starter",
  "version": "1.0.0",
  "description": "Typescript Node Starter",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc -p .",
    "prepare": "husky install",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "lint": "eslint ./src/*.*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nirnejak/typescript-node-starter.git"
  },
  "keywords": [
    "Typescript",
    "Node",
    "Fastify"
  ],
  "author": "Jitendra Nirnejak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nirnejak/typescript-node-starter/issues"
  },
  "homepage": "https://github.com/nirnejak/typescript-node-starter#readme",
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/heapdump": "^0.3.2",
    "@types/node": "^20.6.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@fastify/helmet": "^11.1.1",
    "colorette": "^2.0.20",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.23.2",
    "heapdump": "^0.3.15"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint"
  },
  "volta": {
    "node": "18.16.0"
  }
}
